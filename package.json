{
  "name": "hello-nextjs",
  "version": "0.0.1",
  "description": "a nextjs hello world app",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "node server.js"
  },
  "dependencies": {
    "next": "^7.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "antd": "^3.11.0",
    "babel-plugin-import": "^1.11.0",
    "@zeit/next-less": "^1.0.1",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "less": "^3.9.0",
    "express": "^4.16.4"
  },
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "url": "https://glitch.com/#!/project/hello-nextjs-antd"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  }
}