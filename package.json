{
  "name": "hello-nextjs",
  "version": "0.0.1",
  "description": "a nextjs hello world app",
  "main": "server.js",
  "scripts": {
    "start": "next"
  },
  "dependencies": {
    "next": "^4.2.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "antd": "^3.11.0",
    "babel-plugin-import": "^1.11.0",
    "@zeit/next-less": "^1.0.1"
  },
  "engines": {
    "node": "6.9.x"
  },
  "repository": {
    "url": "https://glitch.com/#!/project/hello-nextjs"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "babel": {
    "presets": ["next/babel"],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  }
}